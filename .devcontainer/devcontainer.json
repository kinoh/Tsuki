{
  "name": "tsuki-core",
  "workspaceFolder": "/workspaces/tsuki",
  "dockerComposeFile": ["../compose.yaml", "../compose.dev.yaml", "./compose.yaml"],
  "service": "core",
  "shutdownAction": "stopCompose",
  "remoteUser": "node",
  "postCreateCommand": "npm -g i pnpm",
  "containerEnv": {
    "NPM_CONFIG_PREFIX": "/home/node/.npm-global",
    "PATH": "/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin:/home/node/.npm-global/bin"
  }
}
